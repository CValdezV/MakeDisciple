        </activity>
        <activity
            android:name=".NoteTaking"
            android:parentActivityName=".TableOfContents">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TableOfContents" />
        </activity>
        <activity
            android:name=".Abbreviations"
            android:parentActivityName=".BookFrame">
            <meta-data
                android:name="anndroid.support.PARENT_ACTIVITY"
                android:value=".BookFrame" />
        </activity>
        <activity android:name=".Introduction"
            android:parentActivityName=".BookFrame">
            <meta-data
